    </section>
    <section class="search-bar find-programs">
      <h2 class="find-programs__title main-title">Like this career? Find programs near you.</h2>
      <form name="search-career" id="search-career-detail">
        <div class="input-group search-group">
          <input class="form-control search-input" id="programAutocompleteCareers" type="text" name="search-input" value="<%= career.title %>"/>
          <div class="form-group btn btn-search btn-region">
            <select class="form-control">
              <option value="" disabled selected>Region</option>
              <% @list_of_regions.each do |region|%>
                <option value="<%= region.name %>"><%= region.name %></option>
              <% end %>
            </select>
            <span class="fa fa-caret-down"></span>
          </div>
          <button type="submit" class="btn btn-search btn-submit">Search</button>
        </div>
      </form>
      <div class="auto-fill-data" id="availableProgramCareer"><%= @list_of_careers.to_json %></div>
    </section>
    <% if @profile %>
      <section class="story-owner row">
        <div class="col-xs-6 col-md-6 pull-right story-owner__content">
          <h2 class="story-owner__title main-title"><%= @profile.first_name + ' ' + @profile.last_name %></h2>
          <!-- <a href="javascript:void(0);" class="story-owner__link"></a> -->
        </div>
        <div class="story-owner__photo">
          <%= image_tag @profile.image_large, lazy: true, class: "lazy-load" %>
        </div>
      </section>
    <% end %>
    <section class="related-careers">
      <% related_by_skills = @related_by_skills %>
      <% related_by_interests = @related_by_interests %>
      <% has_skills  = related_by_skills.present? %>
      <% has_interests  = related_by_interests.present? %>
      <% if has_skills || has_interests %>
        <section class="related-careers__content">
          <div class="related-careers-container">
            <div class="related-careers__header">
              <h2 class="related-careers__title main-title shadow-main-title">Find related careers</h2>
              <div class="row">
                <div class="col-xs-6 col-md-6 related-careers__sort-by">
                  <span class="sort-by">Sort by:</span>
                  <% if has_interests %>
                    <button class="btn btn-sort btn-interest btn-sort-active" id="sortInterests" type="button">Interests</button>
                  <% end %>
                  <% if has_skills %>
                    <% className = has_interests ? 'btn btn-sort btn-skill' : 'btn btn-sort btn-skill btn-sort-active' %>
                    <button class="<%= className %>" id="sortSkills" type="button">Skills</button>
                  <% end %>
                </div>
                <div class="col-xs-6 col-md-6">
                  <% if has_interests %>
                    <div id="interest-icons">
                      <%= render partial: "career/partial/detail-interest",locals: { interests: @career.interests } %>
                    </div>
                  <% end %>
                  <% if has_skills %>
                    <div id="skill-icons">
                      <%= render partial: "career/partial/detail-skill", locals: { skills: @career.skills } %>
                    </div>
                  <% end %>
                </div>
              </div>
            </div>
            <% if has_skills %>
              <div class="related-careers__items row" id="careers-by-skills">
                <%= render partial: "career/partial/related-career-by-skill",locals: { related_by_skills: related_by_skills } %>
              </div>
            <% end %>
            <% if has_interests %>
              <div class="related-careers__items row" id="careers-by-interests">
                <%= render partial: "career/partial/related-career-by-interest", locals: { related_by_interests: related_by_interests } %>
              </div>
            <% end %>
          </div>
        </section>
      <% end %>
      <section class="want-to-talk guided-journey__box career-guided-journey">
        <h2 class="main-title want-to-talk__title">Want to talk?</h2>
        <p class="journey-link want-to-talk__link">
          <span>1-844-LA-CALLS</span>
        </p>
      </section>
    </section>
  </div>

  <%= render "footer/index" %>
</div>
