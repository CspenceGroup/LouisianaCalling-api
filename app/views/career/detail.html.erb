<% career = @career %>
<div class="container-fluid">
  <%= render "navigation/index" %>
  <div class="main-content">
    <section class="career-banner">
      <h2 class="career-banner__title hero-banner__title"><%= career.title%></h2>
      <div class="career-banner__bg">
        <%= image_tag career.photo_large, alt: 'career banner', lazy: true, class: "lazy-load"%>
      </div>
    </section>
    <section class="career-desc" id="career-desc">
      <div class="career-desc__main-container">
        <section class="career-desc__about">
          <h2 class="main-title">About the job</h2>
          <p><%= career.about_job.html_safe %></p>
        </section>
        <section class="career-desc__what-to-do">
          <h2 class="main-title">What you'll do</h2>
          <p><%= career.what_will_do.html_safe %></p>
        </section>
        <!-- <section class="career-desc__social-networks">
          <i class="icon i-social i-facebook" aria-hidden="true"></i>
          <i class="icon i-social i-twitter" aria-hidden="true"></i>
          <a href="mailto:"><i class="icon i-social i-envelope"></i></a>
          <a href="javascript:void(0);"><i class="icon i-social i-print"></i></a>
        </section> -->
      </div>
      <!-- <div class="career-desc__news-container" id="feature-news">
        <section class="career-desc__feature-news">
          <h2>Feature news</h2>
          <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Beatae eos tenetur est quo cum a, esse iusto nemo commodi eius deleniti totam, error nam! Nemo aliquid impedit obcaecati mollitia perspiciatis.</p>
          <a href="javascript:void(0);" class="btn-feature-more"></a>
        </section>
      </div> -->
    </section>
    <section class="great-jobs great-jobs-careers-details">
      <div class="great-jobs__container">
        <div class="great-jobs__content">
          <div class="jobs-head">
            <div class="jobs-head__name">
              <p class="jobs-head__title jobs-head__title-great-jobs" title="<%= career.title%>" ><%= career.title %></p>
              <div class="jobs-head__icon">
                <% career.demand.to_i.times do |i| %>
                    <%= image_tag(asset_path("careers/fire-icon.png"), :alt => "fire icon") %>
                <%
                  end
                %>
              </div>
            </div>
            <div class="jobs-head__content">
              <ul class="jobs-head__desc">
                <li>
                  <p class="jobs-head__desc-text">Interests</p>
                  <div class="jobs-head__desc-icon">
                    <% career.interests.each do |interest, url| %>
                      <% if url.present? %>
                        <%= image_tag(url, :alt => "fire icon") %>
                      <% else %>
                        <%= image_tag("", :alt => "fire icon") %>
                      <% end %>
                    <%
                      end
                    %>
                  </div>
                </li>
                <li>
                  <p class="jobs-head__desc-text">Skills</p>
                  <div class="jobs-head__desc-icon">
                    <% career.skills.each do |skill, url| %>
                      <% if url.present? %>
                        <%= image_tag(url, :alt => "icon computer") %>
                      <% else %>
                        <%= image_tag("", :alt => "fire icon") %>
                      <% end %>
                    <%
                      end
                    %>
                  </div>
                </li>
              </ul>
            </div>
          </div>
          <div class="great-jobs__list-icon">
            <ul class="row great-jobs__row">
              <li class="col-xs-3 col-md-3 list-icon__details list-icon__details--more" title="<%= career.industries.join(', ')%>">
                <%= image_tag(asset_path("careers/icon-industry.png"), :alt => "icon industry", :class => "icon-industry") %>
                <% career.industries.each do |industry| %>
                    <p><%= industry %></p>
                <%
                  end
                %>
              </li>
              <li class="col-xs-3 col-md-3 list-icon__details">
                <%= image_tag(asset_path("careers/icon-salary.png"), :alt => "icon salary", :class => "icon-salary") %>
                <p>$<%= number_with_delimiter(career.salary_min, :delimiter => ',') %> - $<%= number_with_delimiter(career.salary_max, :delimiter => ',')%></p>
              </li>
              <li class="col-xs-3 col-md-3 list-icon__details">
                <%= image_tag(asset_path("careers/icon-training.png"), :alt => "icon training", :class => "icon-training") %>
                <p><%= career.education%></p>
              </li>
              <li class="col-xs-3 col-md-3 list-icon__details list-icon__details--more" title="<%= career.regions_high_demand.join(', ')%>">
                <%= image_tag(asset_path("careers/icon-map.png"), :alt => "icon map", :class => "icon-map") %>
                <% career.regions_high_demand.each do |region| %>
                    <p><%= region %></p>
                <%
                  end
                %>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </section>
    <section class="career-map top-jobs row">
      <div class="data-map-hidden" id="careerMapData"><%= @regions.to_json%></div>
      <div class="jobs-container">
        <div class="career-map__title map-title shadow-main-title">
