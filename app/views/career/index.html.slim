<div>
    = render 'navigation/index'
    <div class="main-content">
      <section class="hero-slider">
        <div id="carousel-banner" class="carousel slide" data-ride="carousel">
          <ol class="carousel-indicators carousel-dots-list">
            <li data-target="#carousel-banner" data-slide-to="0" class="active carousel-dots"></li>
            <li data-target="#carousel-banner" data-slide-to="1" class="carousel-dots"></li>
          </ol>
          <div class="carousel-inner carousel-content" role="listbox">
            <div class="carousel-item active">
              <video controls loop autoplay muted class="video-playing video-player">
                <source src="https://a0.muscache.com/airbnb/static/Belong_p1_v2.mp4">
              </video>
              <div class="carousel-caption carousel-title carousel-title-careers">
                <h1 class="hero-banner__title">Find Your Calling.</h1>
                <h1 class="hero-banner__title">Create Your Legacy.</h1>
                <div class="carousel-text carousel-text-careers">
                  <h2 class="hero-banner__text">Good news: There are tons of careers to choose from.</h2>
                  <h2 class="hero-banner__text">bad news: there are tons of careers to choose from. Let's find the right one for you.</h2>
                </div>
              </div>
            </div>
            <div class="carousel-item">
              <video controls loop autoplay muted class="video-playing video-player">
                <source src="https://a0.muscache.com/airbnb/static/Belong_p1_v2.mp4">
              </video>
              <div class="carousel-caption carousel-title carousel-title-careers">
                <h1 class="hero-banner__title">Find Your Calling.</h1>
                <h1 class="hero-banner__title">Create Your Legacy.</h1>
                <div class="carousel-text carousel-text-careers">
                  <h2 class="hero-banner__text">Good news: There are tons of careers to choose from.</h2>
                  <h2 class="hero-banner__text">bad news: there are tons of careers to choose from. Let's find the right one for you.</h2>
                </div>
              </div>
            </div>
          </div>
          <a class="left carousel-control carousel-control--beside" href="#carousel-banner" role="button" data-slide="prev">
            <span class="icon-prev" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
          </a>
          <a class="right carousel-control carousel-control--beside" href="#carousel-banner" role="button" data-slide="next">
            <span class="icon-next" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
          </a>
        </div>
      </section>
    </div>
    = render 'footer/index'
</div>
