                  <button type="button" class="btn-play-video" data-toggle="modal" data-target="#myModal"></button>
                </div>
              </div>
            </div>
          </div>
          <a class="left carousel-control carousel-control--beside" href="#carousel-banner" role="button" data-slide="prev">
            <span class="icon-prev" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
          </a>
          <a class="right carousel-control carousel-control--beside" href="#carousel-banner" role="button" data-slide="next">
            <span class="icon-next" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
          </a>
        </div>

        <!-- Modal -->
        <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
          <div class="modal-dialog" role="document">
            <div class="modal-content">
              <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                  <span aria-hidden="true">&times;</span>
                </button>
                <h4 class="modal-title" id="myModalLabel">Modal title</h4>
              </div>
              <div class="modal-body">
                abckjansfklanfk
              </div>
              <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary">Save changes</button>
              </div>
            </div>
          </div>
        </div>


      </section>
      <section class="search-bar row">
        <form name="search">
          <div class="input-group search-group">
            <input class="form-control" type="text" name="search-input" placeholder="Search for careers, like 'welding'."/>
            <button type="button" class="btn-search btn-region" data-toggle="dropdown-menu">Region <span class="fa fa-caret-down"></span>
            </button>
            <button type="button" class="btn-search btn-career" data-toggle="dropdown">Find Careers <span class="fa fa-caret-down"></span>
            </button>
            <ul class="dropdown-menu dropdown-menu" role="menu">
            </ul>
            <button type="submit" class="btn-search btn-submit">Search</button>
          </div>
        </form>
      </section>
      <section class="guided-journey row">
        <section class="col-md-6 guided-journey__box find-your-path">
          <h2 class="main-title find-your-path__title">Not sure what to do?</h2>
          <a href="javascript:void(0);" class="journey-link find-your-path__link"></a>
